{
	"name": "PL_Load_MasterTables_BYOD_To IRIS",
	"properties": {
		"activities": [
			{
				"name": "Copy_ProductType_From Byod_To_IRIS",
				"description": "Load ProductType from Byod to IRIS database",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT DISTINCT ProductType FROM dbo.AllPRoducts\nWHERE ProductType NOT IN ('FF&E' ,'') ",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "delete from stg.Byod_ProductType",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": true,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ds_byod",
						"type": "DatasetReference",
						"parameters": {
							"SchemaName": "dbo",
							"TableName": "AllProducts"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "ds_iris",
						"type": "DatasetReference",
						"parameters": {
							"schema": "stg",
							"table": "Byod_ProductType"
						}
					}
				]
			},
			{
